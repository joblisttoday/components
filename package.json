{
  "name": "@joblist/components",
  "private": false,
  "version": "0.2.16",
  "homepage": "https://github.com/joblisttoday/components",
  "keywords": ["web-components", "leaflet", "duckdb", "wasm", "joblist"],
  "author": "joblist",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": "./dist-lib/index.js",
    "./providers": "./src/providers/index.js",
    "./sdk": "./src/libs/sdk.js",
    "./sanitizer": "./src/libs/sanitizer.js",
    "./mwc": "./src/libs/mwc.js",
    "./style": "./dist-lib/index.css"
  },
  "main": "./dist-lib/index.js",
  "files": ["**/*", "dist-lib/*"],
  "scripts": {
    "dev": "vite",
    "build": "npm run build-website; npm run build-lib;",
    "build-lib": "vite build --config ./src/vite/lib.js",
    "build-website": "vite build --config ./src/vite/website.js",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "npx @biomejs/biome lint --write"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.29.0",
    "@observablehq/plot": "^0.6.17",
    "@sctlib/mwc": "^0.3.3",
    "@stripe/stripe-js": "^7.8.0",
    "date-fns": "^4.1.0",
    "giscus": "^1.6.0",
    "isomorphic-dompurify": "^2.26.0",
    "leaflet": "^1.9.4",
    "lucide": "^0.539.0"
  },
  "devDependencies": {
    "recursive-readdir": "^2.2.3",
    "vite": "^7.1.2",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joblisttoday/components.git"
