{
  "name": "@joblist/components",
  "private": false,
  "version": "0.3.0",
  "homepage": "https://github.com/joblisttoday/components",
  "keywords": ["web-components", "leaflet", "duckdb", "wasm", "joblist"],
  "author": "joblist",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": "./dist-lib/index.js",
    "./providers": "./src/providers/index.js",
    "./sdk": "./src/libs/sdk.js",
    "./sanitizer": "./src/libs/sanitizer.js",
    "./style": "./dist-lib/index.css"
  },
  "main": "./dist-lib/index.js",
  "files": ["**/*", "dist-lib/*"],
  "scripts": {
    "dev": "concurrently 'npm run dev:website' 'npm run storybook'",
    "dev:website": "vite",
    "dev:storybook": "storybook dev -p 7007",
    "build": "npm run typecheck && npm run build-website && npm run build-lib && npm run typedoc && npm run build-storybook",
    "build-lib": "vite build --config ./src/vite/lib.js",
    "build-website": "vite build --config ./src/vite/website.js",
    "build-storybook": "storybook build --output-dir dist-website/storybook",
    "preview": "npx serve ./dist-website/",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "biome lint",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 7007",
    "typedoc": "typedoc src/**/*.js --out dist-website/typedoc --options typedoc.json"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.29.0",
    "@observablehq/plot": "^0.6.17",
    "@stripe/stripe-js": "^7.8.0",
    "date-fns": "^4.1.0",
    "giscus": "^1.6.0",
    "isomorphic-dompurify": "^2.26.0",
    "leaflet": "^1.9.4",
    "lucide": "^0.541.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/web-components": "^8.6.14",
    "@storybook/web-components-vite": "^8.6.14",
    "@types/chai": "^4.3.11",
    "@types/emscripten": "^1.39.6",
    "@types/mdx": "^2.0.13",
    "@types/sinon": "^17.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "happy-dom": "^18.0.1",
    "lit": "^3.3.1",
    "recursive-readdir": "^2.2.3",
    "storybook": "^8.6.14",
    "typedoc": "^0.28.10",
    "typedoc-plugin-missing-exports": "^4.1.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joblisttoday/components.git"
  },
  "bugs": {
    "url": "https://github.com/joblisttoday/components/issues"
  }
}
