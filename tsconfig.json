{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "target": "es2020",
    "module": "esnext",
    "lib": ["es2020", "dom", "dom.iterable", "webworker"],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "strict": false,
    "noImplicitReturns": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.d.ts"]
}